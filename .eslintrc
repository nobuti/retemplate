{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "react/display-name": 0
  }
}
